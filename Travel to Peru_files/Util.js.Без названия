function afterEnterGoto(event, id)
{
    if (event.which == 13 || event.keyCode == 13)
    {
    	if (id != null || id != '') { document.getElementById(id).focus(); }
        return false;
    }
}

function validateEmail(email)
{
	var mailFormatServer = /^[-A-Za-z0-9~\!$%^&*_\=+}{'?]+(.[-A-Za-z0-9~\!$%^&*_\=+}{'?]+)*@([a-z0-9_]+(.[-a-z0-9_]+)*.(aero|arpa|biz|com|COM|coop|edu|gov|info|int|mil|museum|name|net|org|pro|[A-Za-z][A-Za-z])|([0-9]{1,3}.[0-9]{1,3}.[0-9]{1,3}.[0-9]{1,3}))(\:[0-9]{1,5})?$/g;
	var mailMask = /^[A-Za-z0-9.@-_]*$/g;

		if(mailFormatServer.test(email) && mailMask.test(email))
		{
			/*var arrobas = new StringTokenizer(email, "@");*/
			var arrobas = email.split("@");

			if(arrobas.length != 2)
			{
				return false;
			}
			else
			{
				var antesArroba = arrobas[0];
				var inicio = antesArroba.substring(0, 1);
				var fin    = antesArroba.substring((antesArroba.length - 1), antesArroba.length);

				if(inicio == "-" || inicio == "_" || fin == "-" || fin == "_")
				{
					return false;
				}
				else
				{
					var despuesArroba = arrobas[1];
					var puntos = despuesArroba.split(".");

					if(puntos.length < 2)
					{
						return false;
					}
					else
					{
						for (var i = 0; i < puntos.length; i++)
						{
							var valor = puntos[i];
							inicio = valor.substring(0, 1);
							fin    = valor.substring((valor.length - 1), valor.length);

							if(inicio == "-" || inicio == "_" || fin == "-" || fin == "_")
							{
								return false;
							}
						}
					}
				}
			}

			return true;
		}
		else
		{
			return false;
		}

		return true;
}

