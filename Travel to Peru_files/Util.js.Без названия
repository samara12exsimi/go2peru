function selectQty(textSelectqtyrequired, vcidlanguage)
{
	var prsinidproductsubGroup = document.getElementById('prsinidproductsubGroup').value;
	var positionInitial = 0;
	var positionEnd = 0;
	var addCart	= "";

	do
	{
		/*OBTENEMOS EL ID DEL SUBPRODUCTO*/
		positionEnd = prsinidproductsubGroup.indexOf(',',positionInitial);
		if (positionEnd == -1) { positionEnd = prsinidproductsubGroup.length; }
		var prsinidproductsub = prsinidproductsubGroup.substring(positionInitial,positionEnd);
		positionInitial = positionEnd + 1;

		var qty = document.getElementById('qty' + prsinidproductsub).value;

		if (qty != null && qty != "" && qty != 0)
		{
			addCart = "" + addCart + prsinidproductsub + "," + qty + ";";
		}
	}
	while (positionInitial <= prsinidproductsubGroup.length);

	if (addCart	== '')
	{
		showMessage(((vcidlanguage != null && vcidlanguage == 'ES') ? 'Mensaje' : 'Message'), textSelectqtyrequired + ".");
		return;
	}

	document.getElementById('bodyForm:prsinidproductsubQty').value = addCart;
	document.getElementById('bodyForm:linkReserve').click();
}

function packageselectQtyTravelers(textSelectqtyrequired, vcidlanguage)
{
	var prsinidproductsubGroup = document.getElementById('prsinidproductsubGroup').value;
	var positionInitial = 0;
	var positionEnd = 0;
	var addCart	= "";

	do
	{
		/*OBTENEMOS EL ID DEL SUBPRODUCTO*/
		positionEnd = prsinidproductsubGroup.indexOf(',',positionInitial);
		if (positionEnd == -1) { positionEnd = prsinidproductsubGroup.length; }
		var prsinidproductsub = prsinidproductsubGroup.substring(positionInitial,positionEnd);
		positionInitial = positionEnd + 1;

		var qty = document.getElementById('qty' + prsinidproductsub).value;
		var pax = document.getElementById('pax' + prsinidproductsub).value;

		if (qty != null && qty != "" && qty != 0 && pax != null && pax != "" && pax != 0)
		{
			addCart = "" + addCart + prsinidproductsub + "," + qty + "," + pax + ";";
		}
	}
	while (positionInitial <= prsinidproductsubGroup.length);

	if (addCart	== '')
	{
		showMessage(((vcidlanguage != null && vcidlanguage == 'ES') ? 'Mensaje' : 'Message'), textSelectqtyrequired + ".");
		return;
	}

	document.getElementById('bodyForm:prsinidproductsubQty').value = addCart;
	document.getElementById('bodyForm:linkReserve').click();
}

function selectTicket(textSelectticketrequired, vcidlanguage, alistListTicketSubCatps1, alistListTicketSubCatps2)
{
	var prsinidproductsubGroup = document.getElementById('prsinidproductsubGroup').value;
	var positionInitial = 0;
	var positionEnd = 0;
	var addCart  = "";
	var addCart1 = "";
	var addCart2 = "";
	
	/*PARA IDA O IDA Y VUELTA*/
	if (alistListTicketSubCatps1 != null && alistListTicketSubCatps1 != "" && alistListTicketSubCatps1.length > 0)
	{
		do
		{
			/*OBTENEMOS EL ID DEL SUBPRODUCTO*/
			positionEnd = prsinidproductsubGroup.indexOf(',',positionInitial);
			if (positionEnd == -1) { positionEnd = prsinidproductsubGroup.length; }
			var prsinidproductsub = prsinidproductsubGroup.substring(positionInitial,positionEnd);
			positionInitial = positionEnd + 1;

			if (document.getElementById('go' + prsinidproductsub) && document.getElementById('go' + prsinidproductsub).checked == true)
			{
				addCart1 = "" + addCart1 + prsinidproductsub + ";";
			}
		}
		while (positionInitial <= prsinidproductsubGroup.length);
	}

	/*PARA IDA O IDA Y VUELTA*/
	positionInitial = 0;
	positionEnd = 0;
	if (alistListTicketSubCatps2 != null && alistListTicketSubCatps2 != "" && alistListTicketSubCatps2.length > 0)
	{
		do
		{
			/*OBTENEMOS EL ID DEL SUBPRODUCTO*/
			positionEnd = prsinidproductsubGroup.indexOf(',',positionInitial);
			if (positionEnd == -1) { positionEnd = prsinidproductsubGroup.length; }
			var prsinidproductsub = prsinidproductsubGroup.substring(positionInitial,positionEnd);
			positionInitial = positionEnd + 1;

			if (document.getElementById('return' + prsinidproductsub) && document.getElementById('return' + prsinidproductsub).checked == true)
			{
				addCart2 = "" + addCart2 + prsinidproductsub + ";";
			}
		}
		while (positionInitial <= prsinidproductsubGroup.length);
	}

	addCart = addCart1 + addCart2;
	if (addCart == '')
	{
		showMessage(((vcidlanguage != null && vcidlanguage == 'ES') ? 'Mensaje' : 'Message'), textSelectticketrequired);
		return;
	}

	document.getElementById('bodyForm:prsinidproductsubQty').value = "" + addCart;
	document.getElementById('bodyForm:linkReserve').click();
}

function selectTicket2(textSelectticketrequired, vcidlanguage, prsinidproductsub)
{
	var addCart = "" + prsinidproductsub + ";";
	if (addCart == null || addCart == '')
	{
		showMessage(((vcidlanguage != null && vcidlanguage == 'ES') ? 'Mensaje' : 'Message'), textSelectticketrequired);
		return;
	}

	document.getElementById('bodyForm:prsinidproductsubQty').value = "" + addCart;
	document.getElementById('bodyForm:linkReserve').click();
}

function eliminarEspaciosCostados()
{
	var sVariable = eliminarEspaciosCostados.arguments[0];
	var nPosicion = 0;

	if (sVariable == null || sVariable == "") { return sVariable; }

	for (var i = 0; i < sVariable.length; i++)
	{
		if (sVariable.charAt(i) == " ")
		{
			nPosicion++;
		}
		else
		{
			break;
		}
	}

	sVariable = sVariable.substring(nPosicion, sVariable.length);
	nPosicion = sVariable.length;
	for (var i = sVariable.length - 1; i >= 0; i--)
	{
		if (sVariable.charAt(i) == " ")
		{
			nPosicion--;
		}
		else
		{
			break;
		}
	}

	sVariable = sVariable.substring(0,nPosicion);
	return sVariable;
}

function returnPage()
{
	document.getElementById('bodyForm:returnPage').click();
}

function edit(name)
{
	var object = document.getElementById(name);

	if (object.getAttribute('readonly') == 'readonly')
	{
		object.removeAttribute('style');
		object.removeAttribute('readonly');
	}
	else
	{
		object.setAttribute('readonly', 'readonly');
		object.setAttribute('style','border:0; background-color:transparent;');
	}
}

function edit2(nameall, sizeall, name1, size1, name2, size2)
{
	var object0 = document.getElementById(nameall);
	var object1 = document.getElementById(name1);
	var object2 = document.getElementById(name2);

	/*if (object0.getAttribute('style') == 'border:0; background-color:transparent;') , en IE es mas datos*/
	if (object1.getAttribute('readonly') == 'readonly' || object2.getAttribute('readonly') == 'readonly')
	{
		/*EDIT*/
		object0.setAttribute('style', 'border:0; display:none; visibility:hidden;');

		object1.removeAttribute('style');
		object1.removeAttribute('readonly');
		if (object1.length > size1) { object1.setAttribute('size', size1); }
		else 						{ object1.setAttribute('size', object1.length); }

		object2.removeAttribute('style');
		object2.removeAttribute('readonly');
		if (object2.length > size2)	{ object2.setAttribute('size', size2); }
		else 						{ object2.setAttribute('size', object2.length); }
	}
	else
	{
		/*NO EDIT*/
		object0.value = object1.value + " " + object2.value;
		object0.setAttribute('style', 'border:0; background-color:transparent;');
		if (object0.length > sizeall) 	{ object0.setAttribute('size', sizeall); }
		else 							{ object0.setAttribute('size', object0.length); }

		object1.setAttribute('readonly', 'readonly');
		object1.setAttribute('style','border:0; display:none; visibility:hidden;');
		object1.removeAttribute('size');

		object2.setAttribute('readonly', 'readonly');
		object2.setAttribute('style','border:0; display:none; visibility:hidden;');
		object2.removeAttribute('size');
	}
}

function edit3(name, size)
{
	var object = document.getElementById(name);

	if (object.getAttribute('readonly') == 'readonly')
	{
		object.removeAttribute('style');
		object.removeAttribute('readonly');
		if (object.length > size)	{ object.setAttribute('size', size); }
		else						{ object.setAttribute('size', object.length); }
	}
	else
	{
		object.setAttribute('readonly', 'readonly');
		object.setAttribute('style','border:0; background-color:transparent;');
		object.removeAttribute('size');
	}
}

function editcart(id, prsinidproductsub, typeGroup, typeReturn)
{
	if (typeGroup == 1 || (typeGroup == 3 && typeReturn == 1))
	{
		/*PARA EL CASO DE HOTELES O TICKETS DE IDA Y VUELTA*/
		var registryfrom = document.getElementById("registryfrom" + id + prsinidproductsub);
		var imgregistryfrom = document.getElementById("imgregistryfrom" + id + prsinidproductsub);
		var registryto = document.getElementById("registryto" + id + prsinidproductsub);
		var imgregistryto = document.getElementById("imgregistryto" + id + prsinidproductsub);
		var pax = document.getElementById("pax" + id + prsinidproductsub);

		if (registryfrom.getAttribute('readonly') == 'readonly')
		{
			registryfrom.removeAttribute('readonly');
			registryfrom.removeAttribute('style');
			imgregistryfrom.removeAttribute('style');
			registryto.removeAttribute('readonly');
			registryto.removeAttribute('style');
			imgregistryto.removeAttribute('style');
			pax.removeAttribute('disabled');
			pax.removeAttribute('style');
		}
		else
		{
			registryfrom.setAttribute('readonly','readonly');
			registryfrom.setAttribute('style','border:0;');
			imgregistryfrom.setAttribute('style','border:0; display:none; visibility:hidden;');
			registryto.setAttribute('readonly','readonly');
			registryto.setAttribute('style','border:0;');
			imgregistryto.setAttribute('style','border:0; display:none; visibility:hidden;');
			pax.setAttribute('disabled','disabled');
			pax.setAttribute('style','border:0;');
		}
	}
	else
	{
			/*PAQUETES O TICKETS(IDA)*/
			var registryfrom = document.getElementById("registryfrom" + id + prsinidproductsub);
			var imgregistryfrom = document.getElementById("imgregistryfrom" + id + prsinidproductsub);
			var pax = document.getElementById("pax" + id + prsinidproductsub);

			if (registryfrom.getAttribute('readonly') == 'readonly')
			{
				registryfrom.removeAttribute('readonly');
				registryfrom.removeAttribute('style');
				imgregistryfrom.removeAttribute('style');
				pax.removeAttribute('disabled');
				pax.removeAttribute('style');
			}
			else
			{
				registryfrom.setAttribute('readonly','readonly');
				registryfrom.setAttribute('style','border:0;');
				imgregistryfrom.setAttribute('style','border:0; display:none; visibility:hidden;');
				pax.setAttribute('disabled','disabled');
				pax.setAttribute('style','border:0;');
			}
	}
}

/*NOTA: LA LOGICA ES DIFERENTE QUE EN AGREGAR, PUESTO QUE AQUI MANEJA IDCARRITO*/
function saveCart(vcidlanguage, textcheckindateshouldnotbegreaterthancheckoutdate, textdeparturedateshouldnotbegreaterthanreturndate)
{
	var idrowprsinidproductsubGroup = document.getElementById('idrowprsinidproductsubGroup').value;
	var positionInitial = 0;
	var positionEnd = 0;
	var addCart	= "";
	var errorAddCart = false;
	var messageerror = "";
	var quantityerror = 0;

	/*REVISAMOS QUE LAS FECHAS SEAN VALIDAS*/
	do
	{
		/*OBTENEMOS EL ID DEL SUBPRODUCTO*/
		positionEnd = idrowprsinidproductsubGroup.indexOf(',',positionInitial);
		if (positionEnd == -1) { positionEnd = idrowprsinidproductsubGroup.length; }
		var idrowprsinidproductsub = idrowprsinidproductsubGroup.substring(positionInitial,positionEnd);
		positionInitial = positionEnd + 1;

		/*REVISAR SI HA INGRESADO UNA RESERVA PARA ESE ID DEL SUBPRODUCTO*/
		/*for(var i=1; i <= 3; i++)*/
		/*{*/
			/*var name = 'idd' + idrowprsinidproductsub;*/
			/*var cell = document.getElementById(name).style.display;*/

			/*if (cell != 'none')*/
			/*{*/
				var prtvcgroup = document.getElementById('prtvcgroup' + idrowprsinidproductsub).value;
				var prschreturn = document.getElementById('prschreturn' + idrowprsinidproductsub).value;

				if (prtvcgroup == 1 || (prtvcgroup == 3 && prschreturn == 1))
				{
					/*HOTEL O TICKET(IDA Y VUELTA)*/
					removeStyle('registryfrom' + idrowprsinidproductsub);
					removeStyle('registryto' + idrowprsinidproductsub);

					var dataRegistryFrom = document.getElementById('registryfrom' + idrowprsinidproductsub).value;
					var dataRegistryTo = document.getElementById('registryto' + idrowprsinidproductsub).value;

					if (dataRegistryFrom != null && dataRegistryFrom != "" && dataRegistryTo != null && dataRegistryTo != "")
					{
						var sRegistryFrom = "";
						var sRegistryTo = "";

						if (vcidlanguage != null && vcidlanguage == 'ES')
						{
							/*IDIOMA ES - DD/MMM/YYYY*/
							sRegistryFrom = "" + dataRegistryFrom.substring(7,11) + getNumberMonth(dataRegistryFrom, vcidlanguage) + dataRegistryFrom.substring(0,2);
							sRegistryTo = "" + dataRegistryTo.substring(7,11) + getNumberMonth(dataRegistryTo, vcidlanguage) + dataRegistryTo.substring(0,2);
						}
						else
						{
							/*IDIOMA US - MMM/DD/YYYY*/
							sRegistryFrom = "" + dataRegistryFrom.substring(7,11) + getNumberMonth(dataRegistryFrom, vcidlanguage) + dataRegistryFrom.substring(4,6);
							sRegistryTo = "" + dataRegistryTo.substring(7,11) + getNumberMonth(dataRegistryTo, vcidlanguage) + dataRegistryTo.substring(4,6);
						}

						var nRegistryFrom = parseInt(sRegistryFrom,10);
						var nRegistryTo = parseInt(sRegistryTo,10);

						if (prtvcgroup == 1 && ((nRegistryFrom > nRegistryTo) || (nRegistryFrom == nRegistryTo)))
						{	/*HOTEL*/
							setErrorStyle('registryfrom' + idrowprsinidproductsub);								
							showMessage(((vcidlanguage != null && vcidlanguage == 'ES') ? 'Mensaje' : 'Message'), textcheckindateshouldnotbegreaterthancheckoutdate);
							return; 
						}
						else if ((prtvcgroup == 3 && prschreturn == 1) && (nRegistryFrom > nRegistryTo))
						{	/*TICKET(IDA Y VUELTA)*/
							setErrorStyle('registryfrom' + idrowprsinidproductsub);							
							showMessage(((vcidlanguage != null && vcidlanguage == 'ES') ? 'Mensaje' : 'Message'), textdeparturedateshouldnotbegreaterthanreturndate);
							return; 							
						}
					}
					else
					{
						/*AQUI SE DA LAS FECHAS DE FROM Y TO SON VACIAS,*/
						/*PERO NO MOSTRAMOS MENSAJES PQ SOLO ESTAMOS VERIFICANDO QUE SEAN FECHAS VALIDAS Y NO VACIAS*/
						/*ES DECIR QUE FROM NO SEA MAYOR A TO*/
					}
				}
			/*}*/
		/*}*/
	}
	while (positionInitial <= idrowprsinidproductsubGroup.length);

	positionInitial = 0;
	positionEnd = 0;
	errorAddCart = false;

	/*PROCEDEMOS A AGREGAR*/
	do
	{
		/*OBTENEMOS EL ID DEL SUBPRODUCTO*/
		positionEnd = idrowprsinidproductsubGroup.indexOf(',',positionInitial);
		if (positionEnd == -1) { positionEnd = idrowprsinidproductsubGroup.length; }
		var idrowprsinidproductsub = idrowprsinidproductsubGroup.substring(positionInitial,positionEnd);
		positionInitial = positionEnd + 1;

		/*REVISAR SI HA INGRESADO UNA RESERVA PARA ESE ID DEL SUBPRODUCTO*/
		/*for(var i=1; i <= 3; i++)*/
		/*{*/
			/*var name = 'idd' + idrowprsinidproductsub;*/
			/*var cell = document.getElementById(name).style.display;*/

			/*if (cell != 'none')*/
			/*{*/
				removeStyle('registryfrom' + idrowprsinidproductsub);
				var idcarrito = document.getElementById('idcarrito' + idrowprsinidproductsub).value;
				var prtvcgroup = document.getElementById('prtvcgroup' + idrowprsinidproductsub).value;
				var prschreturn = document.getElementById('prschreturn' + idrowprsinidproductsub).value;
				var proinidproduct = document.getElementById('proinidproduct' + idrowprsinidproductsub).value;
				var prsinidproductsub = document.getElementById('prsinidproductsub' + idrowprsinidproductsub).value;
				var chainidchannel = document.getElementById('chainidchannel' + idrowprsinidproductsub).value;
				var dataRegistryFrom = document.getElementById('registryfrom' + idrowprsinidproductsub).value;
				var indexPax = document.getElementById('pax' + idrowprsinidproductsub).options.selectedIndex;
				var valuePax = document.getElementById('pax' + idrowprsinidproductsub).options[indexPax].value;
				var dataRegistryTo = "";

				if(dataRegistryFrom == null || dataRegistryFrom == "")
				{
					setErrorStyle('registryfrom' + idrowprsinidproductsub);
					errorAddCart = true;
				}
				else
				{
					if (vcidlanguage != null && vcidlanguage == 'ES')
					{
						/*IDIOMA ES - DD/MMM/YYYY*/
						dataRegistryFrom = "" + dataRegistryFrom.substring(0,2) + "/" + getNumberMonth(dataRegistryFrom, vcidlanguage) + "/" + dataRegistryFrom.substring(7,11);
					}
					else
					{
						/*IDIOMA US - MMM/DD/YYYY*/
						dataRegistryFrom = "" + dataRegistryFrom.substring(4,6) + "/" + getNumberMonth(dataRegistryFrom, vcidlanguage) + "/" + dataRegistryFrom.substring(7,11);
					}
				}


				if (prtvcgroup == 1 || (prtvcgroup == 3 && prschreturn == 1))
				{
					/*HOTEL Y TICKET(IDA Y VUELTA)*/
					removeStyle('registryto' + idrowprsinidproductsub);
					dataRegistryTo = document.getElementById('registryto' + idrowprsinidproductsub).value;

					if(dataRegistryTo == null || dataRegistryTo == "")
					{
						setErrorStyle('registryto' + idrowprsinidproductsub);
						errorAddCart = true;
					}
					else
					{
						if (vcidlanguage != null && vcidlanguage == 'ES')
						{
							/*IDIOMA ES - DD/MMM/YYYY*/
							dataRegistryTo = "" + dataRegistryTo.substring(0,2) + "/" + getNumberMonth(dataRegistryTo, vcidlanguage) + "/" + dataRegistryTo.substring(7,11);
						}
						else
						{
							/*IDIOMA US - MMM/DD/YYYY*/
							dataRegistryTo = "" + dataRegistryTo.substring(4,6) + "/" + getNumberMonth(dataRegistryTo, vcidlanguage) + "/" + dataRegistryTo.substring(7,11);
						}
					}
				}

				if 	   (errorAddCart == false && (prtvcgroup == 1 || (prtvcgroup == 3 && prschreturn == 1)) && dataRegistryFrom != null && dataRegistryFrom != "" && dataRegistryTo != null && dataRegistryTo != "" && valuePax != null && valuePax != "")
				{	/*HOTEL O TICKET(IDA Y VUELTA)*/
					addCart = addCart + idcarrito + "," + chainidchannel + "," + proinidproduct + "," + prsinidproductsub + "," + prtvcgroup + "," + prschreturn + "," + dataRegistryFrom + "," + dataRegistryTo + "," + valuePax + ";";
				}
				else if(errorAddCart == false && prtvcgroup != 1 && prschreturn != 1 && prschreturn != 1 && dataRegistryFrom != null && dataRegistryFrom != "" && valuePax != null && valuePax != "")
				{
					/*PAQUETE O TICKET(IDA)*/
					addCart = addCart + idcarrito + "," + chainidchannel + "," + proinidproduct + "," + prsinidproductsub + "," + prtvcgroup + "," + prschreturn + "," + dataRegistryFrom + "," + dataRegistryTo + "," + valuePax + ";";
				}
			/*}*/
		/*}*/
	}
	while (positionInitial <= idrowprsinidproductsubGroup.length);

	removeStyle2('vcholdernameall', 'bodyForm:vcholdername', 'bodyForm:vcholderlastname');
	removeStyle('bodyForm:vcholderemail');
	var object = document.getElementById('errormessagetc');	object.setAttribute('style','border:0; display:none; visibility:hidden;');

	/*REVISAR QUE HAYA LLENADO LOS DATOS DEL TITULAR DE LA RESERVA*/
	var vcholdername = eliminarEspaciosCostados(document.getElementById('bodyForm:vcholdername').value);
	var vcholderlastname = eliminarEspaciosCostados(document.getElementById('bodyForm:vcholderlastname').value);
	var vcholderemail = eliminarEspaciosCostados(document.getElementById('bodyForm:vcholderemail').value);
	var accepttc = document.getElementById('bodyForm:acceptTC').checked;

	if (errorAddCart == true || addCart == null || addCart == "")
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar las fechas en que las que desear realizar las reservas." + "<br/>"; } else { messageerror += "You must enter the dates that you want to make the booking." + "<br/>"; }
		quantityerror++;
	}
	if (vcholdername == null || vcholdername == "" || vcholderlastname == null || vcholderlastname == "")
	{
		var errorname = '';
		if (vcholdername == null || vcholdername == "")
		{
			errorname = 'bodyForm:vcholdername';
			if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar el nombre." + "<br/>"; } else { messageerror += "First name is required." + "<br/>"; }
			quantityerror++;
		}

		if (vcholderlastname == null || vcholderlastname == "")
		{
			if (errorname == 'bodyForm:vcholdername') 	{ errorname = "all"; }
			else 										{ errorname = 'bodyForm:vcholderlastname'; }
			if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar el apellido." + "<br/>"; } else { messageerror += "Last name is required." + "<br/>"; }
			quantityerror++;
		}

		setErrorStyle2('vcholdernameall', 'bodyForm:vcholdername', 'bodyForm:vcholderlastname', errorname);
	}
	if (vcholderemail == null || vcholderemail == "")
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar el e-mail." + "<br/>"; } else { messageerror += "E-mail is required." + "<br/>"; }
		setErrorStyle('bodyForm:vcholderemail');
		quantityerror++;
	}
	if (!(validateEmail(vcholderemail)))
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "El e-mail no tiene un formato correcto." + "<br/>"; } else { messageerror += "E-mail doesn't have a correct format." + "<br/>"; }
		setErrorStyle('bodyForm:vcholderemail');
		quantityerror++;
	}
	if (accepttc == false)
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe leer y aceptar los T&#233;rminos y Condiciones."; } else { messageerror += "You must read and accept Terms and Conditions."; }
		removeStyle('errormessagetc');
		quantityerror++;
	}

	if (messageerror.length > 1)
	{
		if (quantityerror < 5)
		{
			showMessage(((vcidlanguage != null && vcidlanguage == 'ES') ? 'Mensaje' : 'Message'), messageerror);
		}
		else
		{
			messageerror = ((vcidlanguage != null && vcidlanguage == 'ES') ? 'Debe completar los datos solicitados.' : 'You must complete the information requested.');
			showMessage(((vcidlanguage != null && vcidlanguage == 'ES') ? 'Mensaje' : 'Message'), messageerror);
		}
		return;
	}

	document.getElementById('bodyForm:vcholdername').value = vcholdername;
	document.getElementById('bodyForm:vcholderlastname').value = vcholderlastname;
	document.getElementById('bodyForm:vcholderemail').value = vcholderemail;
	document.getElementById('bodyForm:prsinidproductsubAdd').value = addCart;
	document.getElementById('bodyForm:linkSaveCart').click();
}

function getNumberMonth(date, vcidlanguage)
{
	var numbermonth = "";

	if (date != null)
	{
		var month = "";
		if (vcidlanguage != null && vcidlanguage == 'ES')
		{
			/*DD/MMM/YYYY*/
			month = date.toString().toUpperCase().substring(3,6);
		}
		else
		{
			/*MMM/DD/YYYY*/
			month = date.substring(0,3).toString().toUpperCase();
		}

		if (month == 'ENE' || month == 'JAN') 		{ numbermonth = '01'; }
		else if (month == 'FEB' || month == 'FEB')	{ numbermonth = '02'; }
		else if (month == 'MAR' || month == 'MAR')	{ numbermonth = '03'; }
		else if (month == 'ABR' || month == 'APR')	{ numbermonth = '04'; }
		else if (month == 'MAY' || month == 'MAY')	{ numbermonth = '05'; }
		else if (month == 'JUN'	|| month == 'JUN')	{ numbermonth = '06'; }
		else if (month == 'JUL' || month == 'JUL')	{ numbermonth = '07'; }
		else if (month == 'AGO' || month == 'AUG')	{ numbermonth = '08'; }
		else if (month == 'SEP' || month == 'SEP')	{ numbermonth = '09'; }
		else if (month == 'OCT' || month == 'OCT')	{ numbermonth = '10'; }
		else if (month == 'NOV' || month == 'NOV')	{ numbermonth = '11'; }
		else if (month == 'DIC' || month == 'DEC')	{ numbermonth = '12'; }
	}

	return numbermonth;
}

function findPos(obj)
{
    var curtop = 0;
    if (obj.offsetParent)
    {
        do
        {
            curtop += obj.offsetTop;
        }
        while (obj = obj.offsetParent);
    	return [curtop];
    }
}

function scrollto(id)
{
	window.scroll(0,findPos(document.getElementById(id)));
}

function rowsperpage(value)
{
	var paginationRowsperpage50 = document.getElementById('paginationRowsperpage50').value;

	if (value == paginationRowsperpage50)
	{
		document.getElementById('bodyForm:linkRowsPerpage50').click();
	}
	else
	{
		document.getElementById('bodyForm:linkRowsPerpage20').click();
	}
}

function hotelactiverowsort(id, sortField, sortAscending)
{
	if (sortField == id)
	{
		if (sortAscending == 'true')
		{
			document.getElementById(id).className = 's_filtercriteria filteron';
		}
		else
		{
			document.getElementById(id).className = 's_filtercriteria filterdown filteron';
		}
	}
	else
	{
		document.getElementById(id).className = 's_filtercriteria';
	}
}
/*G2P y EXT*/
function packageactiverowsort(id, sortField, sortAscending)
{
	if (sortField == id)
	{
		if (sortAscending == 'true')
		{
			document.getElementById(id).className = 's_filtercriteria filteron';
		}
		else
		{
			document.getElementById(id).className = 's_filtercriteria filterdown filteron';
		}
	}
	else
	{
		document.getElementById(id).className = 's_filtercriteria';
	}
}

function drawTRBestDeals()
{
	var countRow = parseInt(document.getElementById('countRow').value);

	if (countRow % 3 == 0)
	{
		document.write('</tr>');
	    document.write('<tr>');
		    document.write('<td>&#160;</td>');
		    document.write('<td>&#160;</td>');
		    document.write('<td>&#160;</td>');
	    document.write('</tr>');
	    document.write('<tr>');
	}

	countRow = countRow + 1;
	document.getElementById('countRow').value = countRow;
}

function drawFinalTableBestDeals()
{
			var countRow = parseInt(document.getElementById('countRow').value);
			countRow = countRow - 1;

			if (countRow % 3 == 1)
			{
			    document.write('<td><img width="150" height="100" /></td>');
			    document.write('</tr>');
			    document.write('<tr>');
				    document.write('<td>&#160;</td>');
				    document.write('<td>&#160;</td>');
				    document.write('<td>&#160;</td>');
			    document.write('</tr>');
			}
			else if (countRow % 3 == 2)
			{
			    document.write('<td><img width="150" height="100" /></td>');
			    document.write('<td><img width="150" height="100" /></td>');
			    document.write('</tr>');
			    document.write('<tr>');
				    document.write('<td>&#160;</td>');
				    document.write('<td>&#160;</td>');
				    document.write('<td>&#160;</td>');
			    document.write('</tr>');
			}
}

function drawTRTopDestination()
{
	var countRow = parseInt(document.getElementById('countRow').value);

	if (countRow % 3 == 0)
	{
		document.write('</tr>');
		document.write('<tr>');		
	}

	countRow = countRow + 1;
	document.getElementById('countRow').value = countRow;
}

function registerCardHolderAndPaymentType(vcidlanguage, inidlocationp)
{
	var messageerror = "";
	var quantityerror = 0;


	removeStyle2('vcnameall', 'bodyForm:vcname', 'bodyForm:vclastname');
	removeStyle('bodyForm:vcemail1');
	removeStyle('bodyForm:vcemail2');
	removeStyle3('bodyForm:insex', '100px');
	removeStyle3('bodyForm:inage', '100px');
	removeStyle3('bodyForm:iniddoctype', '100px');
	removeStyle('bodyForm:vcdocnum');
	removeStyle('bodyForm:vcphone1');
	removeStyle3('bodyForm:inidlocationbillingcountry', '147px');
	removeStyle('bodyForm:vcbillingcity');
	removeStyle('bodyForm:vcbillingaddress');
	removeStyle('bodyForm:vcbillingzipcode');
	var object = document.getElementById('errormessagecard'); object.setAttribute('style','border:0; display:none; visibility:hidden;');


	var vcname = eliminarEspaciosCostados(document.getElementById('bodyForm:vcname').value);
	var vclastname = eliminarEspaciosCostados(document.getElementById('bodyForm:vclastname').value);
	var vcemail1 = eliminarEspaciosCostados(document.getElementById('bodyForm:vcemail1').value);
	var vcemail2 = eliminarEspaciosCostados(document.getElementById('bodyForm:vcemail2').value);
	var indexinsex = document.getElementById('bodyForm:insex').options.selectedIndex;
	var indexinage = document.getElementById('bodyForm:inage').options.selectedIndex;
	var indexiniddoctype = document.getElementById('bodyForm:iniddoctype').options.selectedIndex;
	var vcdocnum = eliminarEspaciosCostados(document.getElementById('bodyForm:vcdocnum').value);
	var vcphone1 = eliminarEspaciosCostados(document.getElementById('bodyForm:vcphone1').value);
	var indexinidlocationbillingcountry = document.getElementById('bodyForm:inidlocationbillingcountry').options.selectedIndex;
	var valueinidlocationbillingcountry = document.getElementById('bodyForm:inidlocationbillingcountry').options[indexinidlocationbillingcountry].value;
	var amounttotal = document.getElementById('bodyForm:amounttotal').innerHTML;

	removeStyleHidden(valueinidlocationbillingcountry, inidlocationp);


	var vcbillingstate = null;
	if (valueinidlocationbillingcountry == inidlocationp)
	{	/*LEER EL COMBO*/

		var indexvcbillingstate = document.getElementById('bodyForm:vcbillingstate2').options.selectedIndex;
		if (indexvcbillingstate != null && indexvcbillingstate > 0)
		{
			vcbillingstate = eliminarEspaciosCostados(document.getElementById('bodyForm:vcbillingstate2').options[indexvcbillingstate].value);
		}
	}
	else
	{	/*LEER CAJA DE TEXTO*/

		vcbillingstate = eliminarEspaciosCostados(document.getElementById('bodyForm:vcbillingstate1').value);
	}
	document.getElementById('bodyForm:vcbillingstate').value = vcbillingstate;

	var vcbillingcity = eliminarEspaciosCostados(document.getElementById('bodyForm:vcbillingcity').value);
	var vcbillingaddress = eliminarEspaciosCostados(document.getElementById('bodyForm:vcbillingaddress').value);
	var vcbillingzipcode = eliminarEspaciosCostados(document.getElementById('bodyForm:vcbillingzipcode').value);



	/*VERIFICAR QUE TIPO DE PAGO SE SELECCIONO*/
	var patinidpaymenttypeGroup = document.getElementById('patinidpaymenttypeGroup').value;
	var positionInitial = 0;
	var positionEnd = 0;

	do
	{
		/*OBTENEMOS EL ID SELECCIONADO*/
		positionEnd = patinidpaymenttypeGroup.indexOf(',',positionInitial);
		if (positionEnd == -1) { positionEnd = patinidpaymenttypeGroup.length; }
		var patinidpaymenttype = patinidpaymenttypeGroup.substring(positionInitial,positionEnd);
		positionInitial = positionEnd + 1;

		/*REVISAR SI HA SELECCIONADO DICHO ITEM*/
		if (document.getElementById('id' + patinidpaymenttype).checked == true)
		{
			document.getElementById('bodyForm:patinidpaymenttype').value = patinidpaymenttype;
			break;
		}

		patinidpaymenttype = null;
	}
	while (positionInitial <= patinidpaymenttypeGroup.length);


	/*REVISAR DATOS OBLIGATORIOS*/
	if (vcname == null || vcname == "" || vclastname == null || vclastname == "")
	{
		var errorname = '';
		if (vcname == null || vcname == "")
		{
			errorname = 'bodyForm:vcname';
			if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar el nombre." + "<br/>"; } else { messageerror += "First name is required." + "<br/>"; }
			quantityerror++;
		}

		if (vclastname == null || vclastname == "")
		{
			if (errorname == 'bodyForm:vcname') 				{ errorname = "all"; }
			else 												{ errorname = 'bodyForm:vclastname'; }
			if (vcidlanguage != null && vcidlanguage == 'ES') 	{ messageerror += "Debe ingresar el apellido." + "<br/>"; } else { messageerror += "Last name is required." + "<br/>"; }
			quantityerror++;
		}

		setErrorStyle2('vcnameall', 'bodyForm:vcname', 'bodyForm:vclastname', errorname);
	}
	if (vcemail1 == null || vcemail1 == "")
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar un e-mail." + "<br/>"; } else { messageerror += "Email is required." + "<br/>"; }
		setErrorStyle('bodyForm:vcemail1');
		quantityerror++;
	}
	if (!(validateEmail(vcemail1)))
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "El e-mail no tiene un formato correcto." + "<br/>"; } else { messageerror += "E-mail doesn't have a correct format." + "<br/>"; }
		setErrorStyle('bodyForm:vcemail1');
		quantityerror++;
	}
	/*A solicitud de JD, el e-mail alternativo no es obligatorio*/
	/*if (vcemail2 == null || vcemail2 == "")*/
	/*{*/
	/*	if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar un e-mail alternativo." + "<br/>"; } else { messageerror += "Alternative Email is required." + "<br/>"; }*/
	/*	setErrorStyle('bodyForm:vcemail2');*/
	/*	quantityerror++;*/
	/*}*/
	/*A solicitud de JD, el e-mail alternativo no es obligatorio, por lo que se puso esta siguiente linea*/
	if (vcemail2 != null && vcemail2 != "")
	{
		if (!(validateEmail(vcemail2)))
		{
			if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "El e-mail alternativo no tiene un formato correcto." + "<br/>"; } else { messageerror += "Alternative E-mail doesn't have a correct format." + "<br/>"; }
			setErrorStyle('bodyForm:vcemail2');
			quantityerror++;
		}
	}
	if (indexinsex == null || indexinsex == 0)
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe seleccionar un sexo." + "<br/>"; } else { messageerror += "Sex is required." + "<br/>"; }
		setErrorStyle3('bodyForm:insex', '100px');
		quantityerror++;
	}
	if (indexinage == null || indexinage == 0)
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe seleccionar una edad." + "<br/>"; } else { messageerror += "Age is required." + "<br/>"; }
		setErrorStyle3('bodyForm:inage', '100px');
		quantityerror++;
	}
	if (indexiniddoctype == null || indexiniddoctype == 0)
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe seleccionar un tipo de documento." + "<br/>"; } else { messageerror += "Type of document is required." + "<br/>"; }
		setErrorStyle3('bodyForm:iniddoctype', '100px');
		quantityerror++;
	}
	if (vcdocnum == null || vcdocnum == "")
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar un n&#250;mero de documento." + "<br/>"; } else { messageerror += "Number of document is required." + "<br/>"; }
		setErrorStyle('bodyForm:vcdocnum');
		quantityerror++;
	}
	if (vcphone1 == null || vcphone1 == "")
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar un n&#250;mero de tel&#233;fono." + "<br/>"; } else { messageerror += "Number of phone is required." + "<br/>"; }
		setErrorStyle('bodyForm:vcphone1');
		quantityerror++;
	}
	if (indexinidlocationbillingcountry == null || indexinidlocationbillingcountry == 0)
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe seleccionar un pa&#237;s." + "<br/>"; } else { messageerror += "Country is requiered." + "<br/>"; }
		setErrorStyle3('bodyForm:inidlocationbillingcountry', '147px');
		quantityerror++;
	}
	if (vcbillingstate == null || vcbillingstate == "")
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar un estado." + "<br/>"; } else { messageerror += "State is required." + "<br/>"; }
		setErrorStyleHidden(valueinidlocationbillingcountry, inidlocationp);
		quantityerror++;
	}
	if (vcbillingcity == null || vcbillingcity == "")
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar una ciudad." + "<br/>"; } else { messageerror += "City is required." + "<br/>"; }
		setErrorStyle('bodyForm:vcbillingcity');
		quantityerror++;
	}
	if (vcbillingaddress == null || vcbillingaddress == "")
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar una direcci&#243;n." + "<br/>"; } else { messageerror += "Address is required." + "<br/>"; }
		setErrorStyle('bodyForm:vcbillingaddress');
		quantityerror++;
	}
	if (vcbillingzipcode == null || vcbillingzipcode == "")
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar un c&#243;digo postal." + "<br/>"; } else { messageerror += "Zip code is required." + "<br/>"; }
		setErrorStyle('bodyForm:vcbillingzipcode');
		quantityerror++;
	}
	if (patinidpaymenttype == null || patinidpaymenttype == "")
	{
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe seleccionar un tipo de pago."; } else { messageerror += "Type of payment is required."; }
		removeStyle('errormessagecard');
		quantityerror++;
	}
	if (patinidpaymenttype != null && parseInt(patinidpaymenttype) == 11 && amounttotal != null && parseFloat(amounttotal) > 10000.00)
	{	/*SI EL PAGO ES PAYPAL y EL MONTO DEL PEDIDO PASA DE 10,000.00*/
		if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Para pagar con Paypal, el monto del pedido no debe exceder de $ 10,000.00"; } else { messageerror += "To pay with Paypal, the amount of order must not exceed $ 10,000.00"; }
		quantityerror++;
	}

	if (messageerror.length > 1)
	{
		if (quantityerror < 5)
		{
			showMessage(((vcidlanguage != null && vcidlanguage == 'ES') ? 'Mensaje' : 'Message'), messageerror);
		}
		else
		{
			messageerror = ((vcidlanguage != null && vcidlanguage == 'ES') ? 'Debe completar los datos solicitados.' : 'You must complete the information requested.');
			showMessage(((vcidlanguage != null && vcidlanguage == 'ES') ? 'Mensaje' : 'Message'), messageerror);
		}
		return;
	}

	
	$.msgBox({
		title: getMessage('title', vcidlanguage),
		content: getMessage('gotopayment', vcidlanguage),
		type: "info",
		opacity: 0.7,
		buttons: [{ value: "OK" }],
		success: function (result) 
				 {
					if (result == "OK") 
					{
						document.getElementById('bodyForm:linkRegisterCardHolderAndPaymentType').click();
					}
				 }
		});
}

function showStates(inidlocation, inidlocationp)
{
	if (inidlocation == inidlocationp)
	{	/*COMBO*/
		document.getElementById('bodyForm:vcbillingstate2').removeAttribute('style');
		document.getElementById('bodyForm:vcbillingstate2').value = '0'; /*COMBO*/
		document.getElementById('bodyForm:vcbillingstate1').setAttribute('style','border:0; display:none; visibility:hidden;');
		/*document.getElementById('bodyForm:vcbillingstate2').focus();*/
	}
	else
	{	/*CAJA DE TEXTO*/
		document.getElementById('bodyForm:vcbillingstate1').removeAttribute('style');
		document.getElementById('bodyForm:vcbillingstate1').value = '';	/*CAJA DE TEXTO*/
		document.getElementById('bodyForm:vcbillingstate2').setAttribute('style','border:0; display:none; visibility:hidden;');
		/*document.getElementById('bodyForm:vcbillingstate1').focus();*/
	}
}

function afterClick(inidlocation, inidlocationp)
{
	if (inidlocation == inidlocationp)
	{	/*COMBO*/
		document.getElementById('bodyForm:vcbillingstate2').focus();
	}
	else
	{	/*CAJA DE TEXTO*/
		document.getElementById('bodyForm:vcbillingstate1').focus();
	}
}

function registerPassenger(vcidlanguage)
{
	var sizealistbeanpassengerGroup = document.getElementById('sizealistbeanpassengerGroup').value;
	var positionInitial = 0;
	var positionEnd = 0;
	var messageerror = "";
	var quantityerror = 0;
	var quantityerror1 = 0;
	var quantityerror2 = 0;
	var quantityerror3 = 0;
	var quantityerror4 = 0;
	var quantityerror5 = 0;
	var quantityerror6 = 0;
	var quantityerror7 = 0;

	var i=0;
	do
	{
		/*OBTENEMOS EL ID SELECCIONADO*/
		positionEnd = sizealistbeanpassengerGroup.indexOf(',',positionInitial);
		if (positionEnd == -1) { positionEnd = sizealistbeanpassengerGroup.length; }
		var sizealistbeanpassenger = sizealistbeanpassengerGroup.substring(positionInitial,positionEnd);
		positionInitial = positionEnd + 1;

		var size = parseInt(sizealistbeanpassenger);

		if (size != null && size > 0)
		{
			for (var j=0; j<size; j++)
			{
				removeStyle('bodyForm:data1:' + i + ':data2:' + j + ':vcname');
				removeStyle('bodyForm:data1:' + i + ':data2:' + j + ':vclastname');
				removeStyle3('bodyForm:data1:' + i + ':data2:' + j + ':insex', '90px;');
				/*removeStyle3('bodyForm:data1:' + i + ':data2:' + j + ':inage', '113px;');*/
				removeStyle('bodyForm:data1:' + i + ':data2:' + j + ':vcdatebirth');
				removeStyle3('bodyForm:data1:' + i + ':data2:' + j + ':iniddoctype', '90px;');
				removeStyle('bodyForm:data1:' + i + ':data2:' + j + ':vcdocnum');
				removeStyle3('bodyForm:data1:' + i + ':data2:' + j + ':inidlocationcountry', '142px;');

				var vcname = document.getElementById('bodyForm:data1:' + i + ':data2:' + j + ':vcname').value;
				var vclastname = document.getElementById('bodyForm:data1:' + i + ':data2:' + j + ':vclastname').value;
				var indexinsex = document.getElementById('bodyForm:data1:' + i + ':data2:' + j + ':insex').options.selectedIndex;
				/*var indexinage = document.getElementById('bodyForm:data1:' + i + ':data2:' + j + ':inage').options.selectedIndex;*/
				var vcdatebirth = document.getElementById('bodyForm:data1:' + i + ':data2:' + j + ':vcdatebirth').value;
				var indexiniddoctype = document.getElementById('bodyForm:data1:' + i + ':data2:' + j + ':iniddoctype').options.selectedIndex;
				var vcdocnum = document.getElementById('bodyForm:data1:' + i + ':data2:' + j + ':vcdocnum').value;
				var indexinidlocationcountry = document.getElementById('bodyForm:data1:' + i + ':data2:' + j + ':inidlocationcountry').options.selectedIndex;

				if (vcname == null || vcname == "")
				{
					if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar un nombre." + "<br/>"; } else { messageerror += "Name is required." + "<br/>"; }
					setErrorStyle('bodyForm:data1:' + i + ':data2:' + j + ':vcname');
					quantityerror++;
					quantityerror1++;
				}
				if (vclastname == null || vclastname == "")
				{
					if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar un apellido." + "<br/>"; } else { messageerror += "Last Name is required." + "<br/>"; }
					setErrorStyle('bodyForm:data1:' + i + ':data2:' + j + ':vclastname');
					quantityerror++;
					quantityerror2++;
				}
				if (indexinsex == null || indexinsex == 0)
				{
					if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe seleccionar un sexo." + "<br/>"; } else { messageerror += "Gender is required." + "<br/>"; }
					setErrorStyle3('bodyForm:data1:' + i + ':data2:' + j + ':insex', '90px;');
					quantityerror++;
					quantityerror3++;
				}
				/*if (indexinage == null || indexinage == 0)*/
				/*{*/
				/*	if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe seleccionar una edad." + "<br/>"; } else { messageerror += "Age is required." + "<br/>"; }*/
				/*	setErrorStyle3('bodyForm:data1:' + i + ':data2:' + j + ':inage', '113px;');*/
				/*	quantityerror++;*/
				/*	quantityerror4++;*/
				/*}*/
				if (vcdatebirth == null || vcdatebirth == "")
				{
					if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar una fecha de nacimiento." + "<br/>"; } else { messageerror += "Date of Birth is required." + "<br/>"; }
					setErrorStyle('bodyForm:data1:' + i + ':data2:' + j + ':vcdatebirth');
					quantityerror++;
					quantityerror4++;
				}
				if (indexiniddoctype == null || indexiniddoctype == 0)
				{
					if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe seleccionar un tipo de documento." + "<br/>"; } else { messageerror += "Type of Document is required." + "<br/>"; }
					setErrorStyle3('bodyForm:data1:' + i + ':data2:' + j + ':iniddoctype', '90px;');
					quantityerror++;
					quantityerror5++;
				}
				if (vcdocnum == null || vcdocnum == "")
				{
					if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe ingresar un n&#250;mero de documento." + "<br/>"; } else { messageerror += "Number of Document is required." + "<br/>"; }
					setErrorStyle('bodyForm:data1:' + i + ':data2:' + j + ':vcdocnum');
					quantityerror++;
					quantityerror6++;
				}
				if (indexinidlocationcountry == null || indexinidlocationcountry == 0)
				{
					if (vcidlanguage != null && vcidlanguage == 'ES') { messageerror += "Debe seleccionar una nacionalidad."; } else { messageerror += "Nationality is required."; }
					setErrorStyle3('bodyForm:data1:' + i + ':data2:' + j + ':inidlocationcountry', '142px;');
					quantityerror++;
					quantityerror7++;
				}
			}
		}

		i=i+1;
	}
	while (positionInitial <= sizealistbeanpassengerGroup.length);


	if (messageerror.length > 1)
	{
		if (quantityerror > 4 ||
			quantityerror1 > 1 || quantityerror2 > 1 || quantityerror3 > 1 || quantityerror4 > 1 || quantityerror5 > 1 || quantityerror6 > 1 || quantityerror7 > 1)
		{
			messageerror = ((vcidlanguage != null && vcidlanguage == 'ES') ? 'Debe completar los datos solicitados.' : 'You must complete the information requested.');
			showMessage(((vcidlanguage != null && vcidlanguage == 'ES') ? 'Mensaje' : 'Message'), messageerror);
		}
		else
		{
			showMessage(((vcidlanguage != null && vcidlanguage == 'ES') ? 'Mensaje' : 'Message'), messageerror);
		}
		return;
	}


	document.getElementById('bodyForm:linkRegisterPassenger').click();
}

function showMessage(title, message)
{
		$.msgBox({
			title: title,
			content: message,
			opacity: 0.7
			});
}

function removeStyle(name)
{
	var object = document.getElementById(name);

	if (object.getAttribute('readonly') == 'readonly')
	{
		object.removeAttribute('style');
		object.setAttribute('readonly', 'readonly');
		object.setAttribute('style','border:0; background-color:transparent;');
	}
	else
	{
		object.removeAttribute('style');
	}
}

function removeStyle2(nameall, name1, name2)
{
	var object0 = document.getElementById(nameall);
	var object1 = document.getElementById(name1);
	var object2 = document.getElementById(name2);

	/*if (object0.getAttribute('style') == 'border:0; display:none; visibility:hidden;') en IE no funciona*/
	if(object1.getAttribute('readonly') == 'readonly' || object2.getAttribute('readonly') == 'readonly')
	{
		/*EN ESTE CASO ES: NO EDIT*/
		object0.value = object1.value + " " + object2.value;
		object0.setAttribute('style', 'border:0; background-color:transparent;');
		object0.setAttribute('size', object0.length);

		object1.setAttribute('readonly', 'readonly');
		object1.setAttribute('style','border:0; display:none; visibility:hidden;');
		object1.removeAttribute('size');

		object2.setAttribute('readonly', 'readonly');
		object2.setAttribute('style','border:0; display:none; visibility:hidden;');
		object2.removeAttribute('size');
	}
	else
	{
		/*EN ESTE CASO ES: EDIT*/
		object0.setAttribute('style', 'border:0; display:none; visibility:hidden;');

		object1.removeAttribute('style');
		object1.removeAttribute('readonly');
		object1.setAttribute('size', object1.length);

		object2.removeAttribute('style');
		object2.removeAttribute('readonly');
		object2.setAttribute('size', object2.length);
	}
}

function removeStyle3(name, width)
{
	var object = document.getElementById(name);

	if (object.getAttribute('readonly') == 'readonly')
	{
		object.removeAttribute('style');
		object.setAttribute('readonly', 'readonly');
		object.setAttribute('style','border:0; width:' + width + ';');
	}
	else
	{
		object.removeAttribute('style');
		object.setAttribute('style','width:' + width);
	}
}

function setErrorStyle(name)
{
	var object = document.getElementById(name);

	if (object.getAttribute('readonly') == 'readonly')
	{
		object.removeAttribute('readonly');

	}

	object.removeAttribute('style');
	object.setAttribute('style','background-color:#FBBDB4;');
}

function setErrorStyle2(nameall, name1, name2, errorname)
{
	var object0 = document.getElementById(nameall);
	var object1 = document.getElementById(name1);
	var object2 = document.getElementById(name2);

	/*if (object0.getAttribute('style') == 'border:0; background-color:transparent;') no funcion en IE*/
	if(object1.getAttribute('readonly') == 'readonly' || object2.getAttribute('readonly') == 'readonly')
	{
		/*EDIT*/
		object0.setAttribute('style', 'border:0; display:none; visibility:hidden;');

		object1.removeAttribute('style');
		object1.removeAttribute('readonly');

		object2.removeAttribute('style');
		object2.removeAttribute('readonly');
	}

	if (errorname == name1) 		{ object1.setAttribute('style','background-color:#FBBDB4;'); }
	else if (errorname == name2) 	{ object2.setAttribute('style','background-color:#FBBDB4;'); }
	else 			 				{ object1.setAttribute('style','background-color:#FBBDB4;'); object2.setAttribute('style','background-color:#FBBDB4;');}
}

function setErrorStyle3(name, width)
{
	var object = document.getElementById(name);

	if (object.getAttribute('readonly') == 'readonly')
	{
		object.removeAttribute('readonly');

	}

	object.removeAttribute('style');
	object.setAttribute('style','width:' + width + '; background-color:#FBBDB4;');
}

function removeStyleHidden(inidlocation, inidlocationp)
{
	if (inidlocation == inidlocationp)
	{	/*COMBO*/
		document.getElementById('bodyForm:vcbillingstate2').removeAttribute('style');
		document.getElementById('bodyForm:vcbillingstate1').setAttribute('style','border:0; display:none; visibility:hidden;');
	}
	else
	{	/*CAJA DE TEXTO*/
		document.getElementById('bodyForm:vcbillingstate1').removeAttribute('style');
		document.getElementById('bodyForm:vcbillingstate2').setAttribute('style','border:0; display:none; visibility:hidden;');
	}
}

function setErrorStyleHidden(inidlocation, inidlocationp)
{
	if (inidlocation == inidlocationp)
	{	/*COMBO*/
		document.getElementById('bodyForm:vcbillingstate2').removeAttribute('style');
		document.getElementById('bodyForm:vcbillingstate2').setAttribute('style','background-color:#FBBDB4;');
		document.getElementById('bodyForm:vcbillingstate1').setAttribute('style','border:0; display:none; visibility:hidden;');
	}
	else
	{	/*CAJA DE TEXTO*/
		document.getElementById('bodyForm:vcbillingstate1').removeAttribute('style');
		document.getElementById('bodyForm:vcbillingstate1').setAttribute('style','background-color:#FBBDB4;');
		document.getElementById('bodyForm:vcbillingstate2').setAttribute('style','border:0; display:none; visibility:hidden;');
	}
}

