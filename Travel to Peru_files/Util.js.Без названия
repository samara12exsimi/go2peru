function random_imglink(image1, linkimage1, image2, linkimage2, image3, linkimage3)
{
  var myimages=new Array();
  /*specify random images below. You can have as many as you wish*/
  myimages[1]=image1;
  myimages[2]=image2;
  myimages[3]=image3;

  /*specify corresponding links below*/
  var imagelinks=new Array();
  imagelinks[1]=linkimage1;
  imagelinks[2]=linkimage2;
  imagelinks[3]=linkimage3;

  var ry=Math.floor(Math.random()*myimages.length);

  if (ry==0)
     ry=1;
  	 document.write('<a href='+'"'+imagelinks[ry]+'"'+'><img src="'+myimages[ry]+'" border=0></a>');
}

function random_img(image1, image2, image3)
{
  var myimages=new Array();
  /*specify random images below. You can have as many as you wish*/
  myimages[1]=image1;
  myimages[2]=image2;
  myimages[3]=image3;

  var ry=Math.floor(Math.random()*myimages.length);

  if (ry==0)
     ry=1;
     document.write('<img src="'+myimages[ry]+'" border=0>');
}

function bestdealsTR(proinidproduct, ltglanvcname, provcimage2, comvcnamecorporate, provclogo2, curvcsymbol, prschavcamount2, lorlanvcname, ltglanvcname, prschreturn, textOneway, textRoundtrip)
{
		var countRow = parseInt(document.getElementById('countRow').value);

		if (countRow == 1 || (((countRow - 1) % 3) == 0))
		{
		    document.write('<tr>');				   
		}

	    	document.write('<td>');
		if (countRow % 3 == 0)
		{
		    document.write('<div class="bestDealsImagesPrinc" style="border-right:none;">');				   
		}
		else
		{
		    document.write('<div class="bestDealsImagesPrinc">');
		}
		    
		document.write('<div class="bestDealsImages">');
		document.write('<div class="bestD_image">');
		document.write('<a href="javascript:searchBestDeals(' + proinidproduct + ')" ondblclick="javascript: return dblclick();" title="' + ltglanvcname + '"><img src="' + provcimage2 + '" title="' + ltglanvcname + '" alt="' + ltglanvcname + '" longdesc="' + ltglanvcname + '" width="150" height="100" border="0" /></a>');
		document.write('<div class="bestD_destino">' + ltglanvcname + '</div>');
		document.write('</div>'); 
		document.write('<div class="bestD_descripcion">');
		document.write('<div>');
		document.write('<a href="javascript:searchBestDeals(' + proinidproduct + ')" ondblclick="javascript: return dblclick();" title="' + comvcnamecorporate + '"><img src="' + provclogo2 + '" title="' + comvcnamecorporate + '" alt="' + comvcnamecorporate + '" longdesc="' + comvcnamecorporate + '" width="98" height="20" border="0" /></a>');
		document.write('</div>');
		document.write('<div class="buses_precio" >' + curvcsymbol + '&#160;' + prschavcamount2 + '</div>');
		document.write('<div class="buses_desc2" >' + lorlanvcname + '&#160;-&#160;' + ltglanvcname + '<br/>');
		document.write('<span>');
		
		if (prschreturn == '0')
		{
			document.write('&#160;' + textOneway);
		}              
		else if (prschreturn == '1')
		{
			document.write('&#160;' + textRoundtrip);
		}
		
		document.write('</span>');
		document.write('</div>');
		document.write('</div>');          
		document.write('</div>'); 
		document.write('</div>');
	    document.write('</td>');

    
		if (countRow % 3 == 0)
		{
			document.write('</tr>');
		    document.write('<tr>');
			document.write('<td>&#160;</td>');
			document.write('<td>&#160;</td>');
			document.write('<td>&#160;</td>');
		    document.write('</tr>');
		}
		
		countRow = countRow + 1;
		document.getElementById('countRow').value = countRow;		    
}

function bestdealsTRFinal()
{
	var countRow = parseInt(document.getElementById('countRow').value);	
	countRow = countRow - 1;
	
	if (countRow % 3 == 1)
	{	/*SI RESIDUO == 1 FUE PORQUE 1(COUNTROW)/3 => 10/3 ==> r=1*/ 
	    document.write('<td><img src="images_new/spacer.gif" width="150" height="100" /></td>');
	    document.write('<td><img src="images_new/spacer.gif" width="150" height="100" /></td>');
	    document.write('</tr>');
	    document.write('<tr>');
		    document.write('<td>&#160;</td>');
		    document.write('<td>&#160;</td>');
		    document.write('<td>&#160;</td>');
	    document.write('</tr>');
	}
	else if (countRow % 3 == 2)
	{	/*SI RESIDUO == 2 FUE PORQUE 2(COUNTROW)/3 => 20/3 ==> r=2*/
	    document.write('<td><img src="images_new/spacer.gif" width="150" height="100" /></td>');
	    document.write('</tr>');
	    document.write('<tr>');
		    document.write('<td>&#160;</td>');
		    document.write('<td>&#160;</td>');
		    document.write('<td>&#160;</td>');
	    document.write('</tr>');
	}				
}

function hotelActiveCheckedTypeSearch(typesearch)
{
	if (typesearch == '2')
	{
		/*BUSQUEDA POR NOMBRE*/
		document.getElementById('typesearch1').checked = false;
		document.getElementById('typesearch2').checked = true;
	}
	else
	{
		/*BUSQUEDA POR DESTINO*/
		document.getElementById('typesearch1').checked = true;
		document.getElementById('typesearch2').checked = false;
	}
}

