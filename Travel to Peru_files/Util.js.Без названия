function extProductReservationCalendar(prsinidproductsub, calendardatebegin, calendardateend, prtvcgroup, prschreturn)
{
	ocultarTD('idh' + prsinidproductsub);

	for (var i = 1; i < 4; i++)
	{	
		/*i*/
	      Calendar.setup({
	        inputField : "registryfrom" + i + prsinidproductsub,
	        trigger    : "imgregistryfrom" + i + prsinidproductsub,
	        onSelect   : function() { this.hide(); verifyDate(1); getSubTotal(); } ,
	        dateFormat : "%d/%m/%Y",
	        align	   : "Br",
	        min		   : Calendar.intToDate(calendardatebegin),
	        max		   : Calendar.intToDate(calendardateend)
	      });
	
		if (prtvcgroup == '1' || (prtvcgroup == '3' && prschreturn == '1'))
		{
	      Calendar.setup({
	        inputField : "registryto" + i + prsinidproductsub,
	        trigger    : "imgregistryto" + i + prsinidproductsub,
	        onSelect   : function() { this.hide(); verifyDate(2); getSubTotal(); },
	        dateFormat : "%d/%m/%Y",
	        align	   : "Br",
	        min		   : Calendar.intToDate(calendardatebegin),
	        max		   : Calendar.intToDate(calendardateend)
	      });
	    }
	    
		ocultarTD('idd' + i + prsinidproductsub);
	}
    
    var prsinidproductsubGroup = document.getElementById('prsinidproductsubGroup').value;
    
    if (prsinidproductsubGroup != null && prsinidproductsubGroup != "")
    {
    	prsinidproductsubGroup = prsinidproductsubGroup + ',';
    }
    
    prsinidproductsubGroup = prsinidproductsubGroup + prsinidproductsub;
    document.getElementById('prsinidproductsubGroup').value = prsinidproductsubGroup; 	
}

