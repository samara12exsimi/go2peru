function ticketActiveCheckedReturnproductsub(returnproductsub)
{
	if (returnproductsub == '1')
	{
		/*BUSQUEDA POR ROUND TRIP*/
		document.getElementById('returnproductsub1').checked = false;
		document.getElementById('returnproductsub2').checked = true;
	}
	else
	{
		/*BUSQUEDA POR ONE WAY*/
		document.getElementById('returnproductsub1').checked = true;
		document.getElementById('returnproductsub2').checked = false;
	}
}
