/*G2P y EXT*/
function productReservationCalendar(idrow, prsinidproductsub, calendardatebegin, calendardateend, prtvcgroup, prschreturn)
{
      /*idrow*/
      Calendar.setup({
        inputField : "registryfrom" + idrow + prsinidproductsub,
        trigger    : "imgregistryfrom" + idrow + prsinidproductsub,
        onSelect   : function() { this.hide(); verifyDateNewVersion(1); getSubTotalNewVersion(); } ,
        dateFormat : "%d/%m/%Y",
        align	   : "Br",
        min	       : Calendar.intToDate(calendardatebegin),
        max	       : Calendar.intToDate(calendardateend)
      });

     if (prtvcgroup == '1' || (prtvcgroup == '3' && prschreturn == '1'))
     {
     	Calendar.setup({
          inputField : "registryto" + idrow + prsinidproductsub,
          trigger    : "imgregistryto" + idrow + prsinidproductsub,
          onSelect   : function() { this.hide(); verifyDateNewVersion(2); getSubTotalNewVersion(); },
          dateFormat : "%d/%m/%Y",
          align	     : "Br",
          min	     : Calendar.intToDate(calendardatebegin),
          max	     : Calendar.intToDate(calendardateend)
       });
    }

    document.getElementById('import' + idrow + prsinidproductsub).value = '0.00';				    
    

    var prsinidproductsubGroup = document.getElementById('prsinidproductsubGroup').value;
    if (prsinidproductsubGroup != null && prsinidproductsubGroup != "")
    {
    	prsinidproductsubGroup = prsinidproductsubGroup + ',';
    }
    prsinidproductsubGroup = prsinidproductsubGroup + prsinidproductsub;
    document.getElementById('prsinidproductsubGroup').value = prsinidproductsubGroup; 

    var idrowprsinidproductsubGroup = document.getElementById('idrowprsinidproductsubGroup').value;
    if (idrowprsinidproductsubGroup != null && idrowprsinidproductsubGroup != "")
    {
    	idrowprsinidproductsubGroup = idrowprsinidproductsubGroup + ',';
    }
    idrowprsinidproductsubGroup = idrowprsinidproductsubGroup + idrow + prsinidproductsub;				    
    document.getElementById('idrowprsinidproductsubGroup').value = idrowprsinidproductsubGroup;
}
