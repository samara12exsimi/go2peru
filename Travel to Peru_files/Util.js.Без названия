function getMessage(type, vcidlanguage)
{
	if (type != null && type == 'gotopayment')
	{
		if (vcidlanguage != null && vcidlanguage == 'ES')
		{
			return "Por favor, NO interrumpa el proceso de pago y evite presionar los botones de avance, retroceso o actualizacin de su navegador.";
		}
		else
		{
			return "Please, DO NOT interrupt the payment process and avoid pressing your browser's forward, backward or update buttons.";			
		}
	}
	else if (type != null && type == 'title')
	{
		if (vcidlanguage != null && vcidlanguage == 'ES')
		{
			return "Mensaje";
		}
		else
		{
			return "Message";			
		}
	}
}