function cartCalendar(idcarrito, inidproductsub, dtdateformat2,  dtdateformatbegin2, dtdateformatend2, calendardatebegin, calendardateend, vcgroup, chreturn, inpassengers)
{
	/*idcarrito*/
	/*dateFormat : "%d/%m/%Y", "%b/%d/%Y"*/
      Calendar.setup({
        inputField : "registryfrom" + idcarrito + inidproductsub,
        trigger    : "imgregistryfrom" + idcarrito + inidproductsub,
        onSelect   : function() { this.hide(); cartverifyDateNewVersion(1); cartgetSubTotalNewVersion(); } ,
        dateFormat : dtdateformat2,
        align	   : "Br",
        min		   : Calendar.intToDate(calendardatebegin),
        max		   : Calendar.intToDate(calendardateend)
      });
      document.getElementById('registryfrom' + idcarrito + inidproductsub).value = dtdateformatbegin2;


	if (vcgroup == '1' || (vcgroup == '3' && chreturn == '1'))
	{
      Calendar.setup({
        inputField : "registryto" + idcarrito + inidproductsub,
        trigger    : "imgregistryto" + idcarrito + inidproductsub,
        onSelect   : function() { this.hide(); cartverifyDateNewVersion(2); cartgetSubTotalNewVersion(); },
        dateFormat : dtdateformat2,
        align	   : "Br",
        min		   : Calendar.intToDate(calendardatebegin),
        max		   : Calendar.intToDate(calendardateend)
      });
      document.getElementById('registryto' + idcarrito + inidproductsub).value = dtdateformatend2;
    }
    
	document.getElementById('pax' + idcarrito + inidproductsub).value = inpassengers;


	/*PARA SABER QUE SUBPRODUCTOS HAY*/
    var prsinidproductsubGroup = document.getElementById('prsinidproductsubGroup').value;
    if (prsinidproductsubGroup != null && prsinidproductsubGroup != "")
    {
    	prsinidproductsubGroup = prsinidproductsubGroup + ',';
    }
    prsinidproductsubGroup = prsinidproductsubGroup + inidproductsub;
    document.getElementById('prsinidproductsubGroup').value = prsinidproductsubGroup; 

    
	/*PARA SABER DEL ID DE CARRITO QUE HAY CON SUBPRODUCTO*/
    var idrowprsinidproductsubGroup = document.getElementById('idrowprsinidproductsubGroup').value;
    if (idrowprsinidproductsubGroup != null && idrowprsinidproductsubGroup != "")
    {
    	idrowprsinidproductsubGroup = idrowprsinidproductsubGroup + ',';
    }
    idrowprsinidproductsubGroup = idrowprsinidproductsubGroup + idcarrito + inidproductsub;				    
    document.getElementById('idrowprsinidproductsubGroup').value = idrowprsinidproductsubGroup;
}
/*G2P y EXT*/
function cardholderPreparePaymenttypeStatesEEUU(inidlocationbillingcountry, inidlocationEEUU, vcbillingstate)
{
	/*TIPO DE PAGO*/
    document.getElementById('patinidpaymenttypeGroup').value = '';

	/*MOSTRAR ESTADOS DE EEUU*/				    
	showStates(inidlocationbillingcountry, inidlocationEEUU);
        			
	if (inidlocationbillingcountry == inidlocationEEUU)
	{	/*COMBO*/
		document.getElementById('bodyForm:vcbillingstate2').value = vcbillingstate;	/*COMBO*/
		document.getElementById('bodyForm:vcbillingstate1').value = '';	/*TEXTO*/
	}
	else
	{	/*TEXTO*/
		document.getElementById('bodyForm:vcbillingstate2').value = '0';/*COMBO*/
		document.getElementById('bodyForm:vcbillingstate1').value = vcbillingstate;	/*TEXTO*/
	}
}
